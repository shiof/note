    &lt;span class=&quot;nt&quot;&gt;-p&lt;/span&gt; 9000:9000 &lt;span class=&quot;se&quot;&gt;\&lt;/span&gt;
    &lt;span class=&quot;nt&quot;&gt;-e&lt;/span&gt; sonar.jdbc.username&lt;span class=&quot;o&quot;&gt;=&lt;/span&gt;sonar &lt;span class=&quot;se&quot;&gt;\&lt;/span&gt;
